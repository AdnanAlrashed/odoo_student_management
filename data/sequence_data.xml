<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1"> <!-- noupdate="1" يمنع تحديث التسلسل عند كل ترقية للوحدة -->

        <!-- تعريف تسلسل رقم هوية الطالب -->
        <record id="seq_student_id" model="ir.sequence">
            <field name="name">Student ID Sequence</field>
            <field name="code">student.id.sequence</field>
            <field name="prefix">%(yy)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
